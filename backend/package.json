{
  "name": "balkan-estate-backend",
  "version": "1.0.0",
  "description": "Backend API for Balkan Estate real estate application",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "seed:products": "ts-node src/scripts/seedProducts.ts",
    "seed:agencies": "ts-node src/scripts/seedAgencies.ts",
    "seed:properties": "ts-node src/scripts/seedMockProperties.ts",
    "test:agencies": "ts-node src/scripts/testAgencyAccess.ts",
    "test:find-agency": "ts-node src/scripts/testFindAgencyByCode.ts",
    "sync:agents": "ts-node src/scripts/syncAgents.ts",
    "cleanup:sold": "ts-node src/scripts/archiveSoldProperties.ts",
    "cleanup:conversations": "ts-node src/scripts/cleanupConversations.ts",
    "remove:agents": "ts-node src/scripts/removeAllAgents.ts",
    "migrate:agency-owners": "ts-node src/scripts/addOwnersToAgencyMembers.ts",
    "init:stats": "ts-node src/scripts/initializeAllUserStats.ts",
    "sync:stats": "ts-node src/scripts/syncAllSellerStats.ts",
    "set-admin": "ts-node src/scripts/setAdminRole.ts",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "real-estate",
    "api",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@types/nodemailer": "^7.0.3",
    "@types/passport": "^1.0.17",
    "@types/passport-facebook": "^3.0.4",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/socket.io": "^3.0.1",
    "@types/stripe": "^8.0.416",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.3.0",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "stripe": "^19.3.1"
